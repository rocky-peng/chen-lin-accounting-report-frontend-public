{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "upgrade-insecure-requests"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/backend/(.*)",
      "destination": "http://101.35.218.107:9088/backend/$1"
    }
  ]
}
